<div class="stream">
  <div class="new-resource">
    <div class="row">
      <div class="col-xs-6">
        <ul class="nav nav-pills">
          <li role="presentation" class="active"><a href="#">Home</a></li>
          <li role="presentation"><a href="#">Profile</a></li>
          <li role="presentation"><a href="#">Messages</a></li>
        </ul>
      </div>
      <div class="col-xs-6">
        <%= link_to new_event_path, class: 'btn btn-sm btn-primary pull-right' do %>
          <i class="icon-plus"></i> New Event
        <% end %>
      </div>
    </div>
  </div>
  <div id="events" class="resources-block newsfeed">
    <h4 class="underlined">EVENTS</h4>
    <%= render(@events) || render('shared/no_resource', resources: 'Events') %>
  </div>
</div>
